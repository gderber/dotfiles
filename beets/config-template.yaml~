# directories that cover
# different field values
# ranges.
#
##################################################
bucket:
########################################
#
# Ranges to use for all substitutions
# occurring on textual fields.
#
# default: none
#
########################################
# bucket_alpha:
########################################
#
# A range: regex mapping (one per line)
# where range is one of the bucket_alpha
# ranges and value is a regex that
# overrides original range definition.
#
# Default: none
#
########################################
# bucket_alpha_regex:
########################################
#
# Ranges to use for all substitutions
# occurring on the $year field.
#
# default: none
#
########################################
  bucket_year: [ '1930s', '1940s', '1950s', '1960s', '1970s', '1980s', '1990s', '2000s', '2010s']
########################################
#
# To group your files into multiple year
# ranges without enumerating them all.
#
# options: yes, no
# default: no
#
########################################
# extrapolate: yes


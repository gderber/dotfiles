##################################################
#
# Alternatives
#
# Manage External Files
#
##################################################
alternatives:
  by-year:
    directory: Alt/Year
    paths:
      default: %bucket{$original_year}/%the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
    formats: link
  by-genre:
    directory: Alt/Genre
    paths:
      genre::^$: Unknown/%the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
      singleton: $genre/%the{%asciify{$albumartist}}/Non-Album/%the{%asciify{$title}}
      comp: $genre/%the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title} 
      default: $genre/%the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
    formats: link
  myplayer:
    directory: Alt/Portable
    paths:
      singleton:true: %the{%asciify{$albumartist}}/Non-Album/%the{%asciify{$title}}
      comp: %the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
      default: %the{%asciify{$albumartist}}/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
    formats: mp3
    query: "onplayer:true"

##################################################

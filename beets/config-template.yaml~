# Default: no
#
########################################
  auto: no

##################################################
#
# MusicBrainz Submit
#
# Print an album’s tracks in a
# MusicBrainz-friendly format.
#
##################################################
mbsubmit:
########################################
#
# The format used for printing the
# tracks, defined using the same
# template syntax as beets’ path
# formats.
#
# Default:
#   $track. $title - $artist ($length)
#
########################################
#  format: $track. $title - $artist ($length)
########################################
#
# The minimum strength of the autotagger
# recommendation that will cause the
# Print tracks choice to be displayed on
# the prompt.
#
# options: none, low, medium, strong
# Default: medium
#
########################################
  threshold: medium

##################################################
#
# Missing
#
# List missing tracks.
#
##################################################
missing:
########################################
#
# Print a count of missing tracks per
# album, with format defaulting to
# $albumartist - $album: $missing.
#
# options: yes,no
# default: no
#
########################################
#  count: yes
########################################
#
# A specific format with which to print
# every track. This uses the same
# template syntax as beets’ path
# formats. 
#
# default: $format_item
#
########################################
#  format: $albumartist - $album - $track - $title
########################################
#
# Print a single count of missing tracks
# in all albums
#
# options: yes,no
# default: no
#
########################################
#  total: yes


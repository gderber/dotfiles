  comp: Various Artists/%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}
  default: %the{%asciify{$albumartist}}/%ifdef{series,$series}/%ifdef{series_no,$series_no - }%the{%asciify{$album}}%aunique{}/%if{$multidisc,%if{$lmultidisc,$disc,%right{$disc,1}}}$track - %asciify{$title}



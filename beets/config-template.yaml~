########################################
#
# Re-analyze files that
# already have ReplayGain
# tags
#
# options: yes, no
# default: no
#
########################################
  overwrite: yes
########################################
#
# A number of decibels for the
# target loudness level
#
# default: 89
#
########################################
  targetlevel: 89
########################################
#
# Only works with command
# backend
#
# The path to the mp3gain or
# aacgain
#
# default: $PATH
#
########################################
#  command:
########################################
#
# Only works with command
# backend
#
# Reduce the amount of
# ReplayGain adjustment to
# whatever amount would keep
# clipping from occurring
#
# options: yes, no
# default: yes
#
########################################
#  noclip: no
########################################
#
# Only works with bs1770gain
# backend
#
# The loudness scanning standard
#
# options: replaygain, ebu, atsc
# default: replaygain
#
########################################
#  method: replaygain


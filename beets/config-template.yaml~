##################################################
#
# FetchArt
#
# Fetch album cover art from
# various sources.
#
##################################################
fetchart:
########################################
#
# Enable automatic album art
# fetching during import.
#
# options: yes, no
# default: yes
########################################
  auto: yes
########################################
#
# Pick only trusted album art
# by ignoring filenames that
# do not contain one of the
# keywords in cover_names
#
# options: yes, no
# default: no
#
########################################
  cautious: yes
########################################
#
# Prioritize images containing
# words in this list
#
# default:
# cover front art album folder
#
########################################
  cover_names: cover front art album folder
########################################
#
# Only images with a width
# bigger or equal to minwidth
# are considered as valid album
# art candidates.
#
# default:0
#
########################################
  minwidth: 480
########################################
#
# A maximum image width to
# downscale fetched images if
# they are too big.
#
# default:
#   unknown
# 
########################################
  maxwidth: 0
########################################
#
# Consider only images with a
# width:height ratio of 1:1 as
# valid
#
# options: yes, no, ##px, ##.#%
# default: no
#
########################################
  enforce_ratio: no
########################################
#
# List of sources to search
# for images.
#
# options:
#   filesystem,
#   coverart,
#   itunes,
#   amazon,
#   albumart,
#   wikipedia,
#   google,
#   fanarttv
# default:
#   filesystem coverart itunes amazon albumart
#
########################################
  sources: filesystem coverart itunes amazon albumart

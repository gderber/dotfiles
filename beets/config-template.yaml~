lastgenre:
########################################
#
# Fetch genres automatically
# during import
#
# options: yes, no
# default: yes
#
########################################
  auto: yes
########################################
#
# Use a canonicalization tree.
#
# options: yes, no
# default: no
#
########################################
  canoncical: no
########################################
#
# Number of genres to fetch.
#
# default:1
#
########################################
  count: 1
########################################
#
#  A string if to use a
# fallback genre when no genre
# is found.
#
# default: none
#
########################################
  fallback: '' 
########################################
#
# always fetch new genres
#
# options: yes, no
# default: yes
#
########################################
  force: yes
########################################
#
# Minimum popularity factor
# below which genres are
# discarded
#
# default: 10
#
########################################
  min_weight: 10
########################################
#
# Sort genres by the most to
# least specific, rather than
# most to least popular.
#
# options: yes, no
# default: no
#
########################################
#  prefer_specific: yes
########################################
#
# Which entity to look up in
# Last.fm
#
# options:
#   artist
#   album
#   track
# default:
#   album
#

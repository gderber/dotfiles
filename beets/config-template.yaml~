##################################################
#
# Convert
#
# Transcode music and embed
# album art while exporting to
# a different directory.
#
##################################################
convert:
########################################
#
# Import transcoded versions of your
# files automatically during imports.
#
# options: yes, no
# default: no
#
########################################
#  auto: yes
########################################
#
# The directory where temporary files
# will be stored during import.
#
# Default: none (system default),
#
########################################
#  tmpdir:
########################################
#
# Copy album art when copying or
# transcoding albums matched using the
# -a option.
#
# options: yes,no
# Default: no.
#
########################################
#  copy_album_art: yes
########################################
#
# Downscale album art if it’s too big.
# The resize operation reduces image
# width to at most maxwidth pixels
# while preserving the aspect ratio.
#
########################################
#  album_art_maxwidth:
########################################
#
# The directory where the files will be
# converted (or copied) to.
#
# Default: none.
#
########################################
  dest: ~/GDrive/Business/Music
########################################
#
# Embed album art in converted items.
#
# options: yes,no
# Default: yes.
#
########################################
#  embed: no
########################################
#
# All lossy files with a higher bitrate
# will be transcoded and those with a
# lower bitrate will simply be copied.
#
# default: none
#
########################################
#  max_bitrate:
########################################
#
# Cross-conversions between lossy
# codecs—such as mp3, ogg vorbis
#
# yes converts
# no copies without converting
#
# options: yes,no
# Default: no
#
########################################
#  never_convert_lossy_files: yes
########################################
#
# The directory structure and naming
# scheme for the converted files.
#
# default: Reuse your top-level path
# format settings.
#
########################################
#  paths:
########################################
#
# Prevent the plugin from announcing
# every file it processes.
#
########################################
#  quiet: true
########################################
#
# The number of threads to use for
# parallel encoding
#
# default: detect the number of
# processors available and use them all.
#
########################################
#  threads:
########################################
#
# default format to convert to
#
########################################
  format: mp3
########################################
#
# available formats to convert to
#
########################################
  formats:
    mp3:
      command: ffmpeg -i $source -y -codec:a libmp3lame -q:a 0 $dest
      extention: mp3
    wav: ffmpeg -i $source -y -acodec pcm_s16le $dest


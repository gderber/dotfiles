  multidisc: 1 if disctotal > 1 else 0
  lmultidisc: 1 if disctotal > 9 else 0

##################################################
#
# Rewrite
#
# Substitute values in path
# formats.
#
##################################################
rewrite:
 artist .*zimmer*.: Hans Zimmer

##################################################
#
# The
#
# Move patterns in path
# formats
#
##################################################
the:
########################################
#
# Handle “A/An” moves.
#
# Options: yes, no
# Default: yes.
#
########################################
  a: yes
########################################
#
# handle “The” moves.
#
# options: yes, no
# Default: yes
#
########################################
  the: yes
########################################
#
# Custom regexp patterns,
# space-separated. Custom patterns are
# case-insensitive regular expressions.
# Patterns can be matched anywhere in
# the string (not just the beginning),
# so use ^ if you intend to match
# leading words.
#
# default: []
#
########################################
  patterns: []
########################################
#
# Remove the article altogether instead
# of moving it to the end.
#
# options: yes,no
#
########################################
  strip: no
########################################
#
# A Python format string for the output.
# Use {0} to indicate the part without
# the article and {1} for the article.
# Spaces are already trimmed from ends
# of both parts. Default: '{0}, {1}'
#
########################################
  format: '{0}, {1}'


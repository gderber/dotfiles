Host *
    StrictHostKeyChecking ask
    UpdateHostKeys ask
    SendEnv WINDOW
    SendEnv LANG LC_*
    CanonicalizeHostname yes

Host *.drbr.org
    ForwardAgent yes
    CertificateFile ~/.ssh/%u-cert.pub
    VisualHostKey yes

Host angelia aphrodite apollo ariadne atlas calliope calypso demeter dionysus hades hera hestia iris morpheus nyx orion persephone phoenix telete thysa zeus
    HostName %h.olympus.drbr.org

Host ha
    HostName hestia.olympus.drbr.org
    User homeassistant
    CertificateFile ~/.ssh/%u-cert.pub

Host mnemosyne
    HostName %h.olympus.drbr.org
    ForwardX11 yes

Host defiance endeavor homeone independence liberty redemption
    HostName %h.swg.drbr.org

Host deathstar
    HostName deathstar.swg.drbr.org
    User cranberry

Host drbr
     HostName drbr.org

#Host git
#     User git
#     Hostname git.drbr.org

Host gh
     User git
     Hostname github.com

Host hm
    HostName hearthminion.com
    User hearthmi
    StrictHostKeyChecking yes
    VerifyHostKeyDNS ask
    VisualHostKey yes

Host local localhost
     HostName localhost
